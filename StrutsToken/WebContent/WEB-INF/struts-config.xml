<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
		<form-bean name="dyForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="name" type="java.lang.String" />
			<form-property name="beginDate" type="java.lang.String" />
			<form-property name="endDate" type="java.lang.String" />
		</form-bean>
	</form-beans>
	
	<action-mappings>
		<action parameter="dispatch" name="dyForm" path="/testtoken" type="kirk.sample.token.TokenTestAction">
			<forward name="goInsert" path="/Insert.jsp"/>
			<forward name="showResult" path="/ShowResult.jsp"/>
		</action>
	</action-mappings>
	
</struts-config>
